<?xml version="1.0" encoding="UTF-8"?>

<project>
  <pomVersion>3</pomVersion>
  <extend>${base.project.dir}/base-project.xmlend>
  <name>MyFaces</name>
  <groupId>bundles</groupId>
  <artifactId>net.sourceforge.myfaces</artifactId>
  <currentVersion>1.0.5</currentVersion>
  <organization/>
  <inceptionYear>2003</inceptionYear>
  <shortDescription>MyFaces JSF Implementation</shortDescription>
  <description>MyFaces is an implementation of JavaServer Faces</description>
  <issueTrackingUrl>http://issues.idega.is</issueTrackingUrl>
  <versions>
    <version>
      <id>1.0.5</id>
      <name>1.0.5</name>
      <tag>MYFACES_1_0_5</tag>
    </version>
  </versions>
  <branches/>
  <developers>
    <developer>
      <name>Tryggvi Larusson</name>
      <id>tryggvil</id>
      <email>tryggvi@idega.is</email>
      <organization>idega software</organization>
      <roles/>
    </developer>
  </developers>
  <dependencies>
    <dependency>
      <groupId>myfaces</groupId>
      <artifactId>myfaces</artifactId>
      <version>1.0.5</version>
      <type>jar</type>
      <url>http://www.myfaces.org/</url>
      <properties>
        <idegaweb.bundle.include>true</idegaweb.bundle.include>
      </properties>
    </dependency>
    <dependency>
      <groupId>jdom</groupId>
      <artifactId>jdom</artifactId>
      <version>b9-rc2</version>
      <type>jar</type>
      <url>http://www.jdom.org/</url>
      <properties/>
    </dependency>
    <dependency>
      <groupId>servletapi</groupId>
      <artifactId>servletapi</artifactId>
      <version>2.3</version>
      <type>jar</type>
      <url>http://java.sun.com/products/servlet/docs.html</url>
      <properties/>
    </dependency>
    <dependency>
      <groupId>cos</groupId>
      <artifactId>cos</artifactId>
      <version>05Nov2002</version>
      <type>jar</type>
      <url>http://www.servlets.com/cos/</url>
      <properties/>
    </dependency>
    <dependency>
      <groupId>jsf</groupId>
      <artifactId>jsf-api</artifactId>
      <version>1.0</version>
      <type>jar</type>
      <url>http://java.sun.com/j2ee/javaserverfaces/</url>
      <properties/>
    </dependency>
  </dependencies>
  <build>
    <sourceDirectory>src/java</sourceDirectory>
    <unitTestSourceDirectory>src/test</unitTestSourceDirectory>
    <unitTest>
      <includes>
        <include>**/*Test.java</include>
      </includes>
    </unitTest>
  </build>
  <properties/>
</project>
